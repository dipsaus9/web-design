{
  "name": "week2",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node ./node/server.js",
    "dev": "concurrently \"npm run serverWatch\" \"npm run  watch-css\" \"npm run browserifyRun\"",
    "build-css": "node-sass src/scss/app.scss public/css/app.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "browserify": "browserify ./src/js/app.js > public/js/app.js",
    "browserifyRun": "nodemon --watch ./src/js -x \"npm run browserify\"",
    "serverWatch": "nodemon --watch ./node -x \"npm start\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.2",
    "browserify": "^16.1.1",
    "compression": "^1.7.2",
    "ejs": "^2.5.7",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-validator": "^5.0.3",
    "http": "0.0.0",
    "mapbox": "^1.0.0-beta9",
    "multer": "^1.3.0",
    "node-fetch": "^2.1.1",
    "node-sass": "^4.7.2",
    "request": "^2.85.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.2"
  },
  "nodemonConfig": {
    "delay": "1000"
  }
}
